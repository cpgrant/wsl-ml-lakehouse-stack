# Copy this to .env and edit values (DO NOT COMMIT .env)
TZ=UTC
AWS_REGION=us-east-1

# MinIO root creds (dev defaults are fine)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin
MINIO_BUCKET=smoketest-bucket

# Jupyter token for pydev
JUPYTER_TOKEN=letmein

# Airflow recommended UID for official image
AIRFLOW_UID=50000

# Optional: MinIO service-account creds (if you use them)
# MINIO_SA_ACCESS=
# MINIO_SA_SECRET=
